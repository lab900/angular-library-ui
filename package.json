{
  "name": "angular-library-ui",
  "version": "13.0.0",
  "repository": "https://github.com/lab900/angular-library-ui",
  "homepage": "https://lab900.github.io/angular-libraries",
  "author": "Lab900 <info@lab900.com> (https://lab900.com)",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4900",
    "build": "ng build",
    "build:ui": "ng build ui",
    "build:ui:prod": "ng build ui --configuration production",
    "watch:ui": "npm run build:ui -- --watch",
    "deploy:showcase": "ng deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty:check": "prettier -c \"{src,lib}/**/**.{ts,js}\"",
    "pretty:fix": "prettier  --write \"{src,lib}/**/**.{ts,js}\"",
    "pretty:html:fix": "prettier --no-config --print-width=140 --write \"{src,lib}/**/**.html\"",
    "clean": "rm -rf node_modules/ && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ng lint --fix"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,css,scss,html,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "~13.3.12",
    "@angular/cdk": "~13.3.9",
    "@angular/common": "~13.3.12",
    "@angular/compiler": "~13.3.12",
    "@angular/core": "~13.3.12",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.12",
    "@angular/material": "~13.3.9",
    "@angular/platform-browser": "~13.3.12",
    "@angular/platform-browser-dynamic": "~13.3.12",
    "@angular/router": "~13.3.12",
    "@kolkov/angular-editor": "2.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@types/nouislider": "^15.0.0",
    "lodash": "^4.17.20",
    "memo-decorator": "^2.0.1",
    "moment": "^2.29.4",
    "ngx-markdown": "^13.1.0",
    "ngx-mask": "13.1.15",
    "nouislider": "^15.7.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.11",
    "@angular/compiler-cli": "~13.3.12",
    "@angular/language-service": "~13.3.12",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "angular-cli-ghpages": "^1.0.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^13.3.1",
    "prettier": "2.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.4"
  }
}
