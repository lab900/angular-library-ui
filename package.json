{
  "name": "angular-library-ui",
  "version": "1.0.0",
  "repository": "https://github.com/lab900/angular-library-ui",
  "homepage": "https://lab900.github.io/angular-libraries",
  "author": "Lab900 <info@lab900.com> (https://lab900.com)",
  "licens": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4900",
    "build": "ng build",
    "build:ui": "ng build ui",
    "build:ui:prod": "ng build ui --configuration production",
    "watch:ui": "npm run build:ui -- --watch",
    "deploy:showcase": "ng deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty:check": "prettier -c \"{src,lib}/**/**.{ts,js}\"",
    "pretty:fix": "prettier  --write \"{src,lib}/**/**.{ts,js}\"",
    "pretty:html:fix": "prettier --no-config --print-width=140 --write \"{src,lib}/**/**.html\"",
    "clean": "rm -rf node_modules/ && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "ng lint --fix"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,css,scss,html,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "^6.0.3",
    "@angular/animations": "~12.2.14",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.14",
    "@angular/material": "~12.2.13",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@kolkov/angular-editor": "^1.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@types/nouislider": "^9.0.7",
    "lodash": "^4.17.20",
    "memo-decorator": "^2.0.1",
    "moment": "^2.29.0",
    "ngx-markdown": "^12.0.1",
    "ngx-mask": "12.0.0",
    "nouislider": "^14.7.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.14",
    "@angular/language-service": "~12.2.14",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^12.2.5",
    "prettier": "2.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.5"
  }
}
